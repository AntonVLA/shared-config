	editor = nano

[color]
	status = true
	branch = true
	diff = true
	interactive = true

[alias]
	co = checkout
	st = status
	br = branch
	fa = "! git fetch --all --prune; git fetch --tags"
	gi = "!gi() { curl http://www.gitignore.io/api/$@ ;}; gi"

[push]
	default = simple


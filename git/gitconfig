[core]
	editor = nano

[color]
	branch      = true
	diff        = true
	interactive = true
	status      = true

[alias]
	co = checkout
	st = status
	br = branch
	fa = "! git fetch --all --prune; git fetch --tag"
	gi = "!gi() { curl http://www.gitignore.io/api/$@ ;}; gi"

[push]
	default = simple

[include]
	path = .gitprivateconfig
